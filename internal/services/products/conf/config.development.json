{
    "service": {
        "name": "products",
        "version": "0.0.1",
        "protocol": [
            "http",
            "graphql",
            "grpc",
            "wss"
        ]
    },
    "telemetry": {
        "service": "products",
        "host": "localhost",
        "port": 5775,
        "insecure": true
    },
    "logger": {
        "level": "debug",
        "type": "zap",
        "fileLogging": true,
        "accessLog": "logs/access.log",
        "errorLog": "logs/error.log"
    },
    "echo": {
        "port": ":3000",
        "host": "localhost",
        "readTimeout": "10s",
        "writeTimeout": "10s",
        "BaseRoute": "/api",
        "debugMode": true
    },
    "sql": {
        "host": "localhost",
        "name": "mysql",
        "port": 3306,
        "user": "user",
        "password": "password",
        "dbName": "mydatabase",
        "sslMode": "disable",
        "ConnStr": "user:password@tcp(localhost:3306)/mydatabase?parseTime=true",
        "maxIdleConns": 10,
        "maxOpenConns": 10,
        "maxLifeTime": 3,
        "maxIdleTime": 3
    },
    "graphql": {
        "port": ":3001",
        "host": "localhost",
        "readTimeout": "10s",
        "writeTimeout": "10s",
        "BaseRoute": "/graphql",
        "debugMode": true
    }
}