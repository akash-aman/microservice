{
    "service": {
        "name": "products",
        "version": "0.0.1",
        "protocol": [
            "http",
            "graphql",
            "grpc",
            "wss"
        ]
    },
    "telemetry": {
        "service": "products",
        "host": "localhost",
        "grpc": ":4317",
        "http": ":4318",
        "port": ":4317",
        "insecure": true
    },
    "logger": {
        "level": "debug",
        "type": "zap",
        "fileLogging": true,
        "encoding": "json",
        "accessLog": "logs/access.log",
        "errorLog": "logs/error.log"
    },
    "echo": {
        "port": ":5004",
        "host": "localhost",
        "readTimeout": "10s",
        "writeTimeout": "10s",
        "BaseRoute": "/api",
        "debugMode": true
    },
    "sql": {
        "host": "localhost",
        "name": "mysql",
        "port": 3306,
        "user": "user",
        "password": "password",
        "dbName": "mydatabase",
        "sslMode": "disable",
        "ConnStr": "user:password@tcp(localhost:3306)/mydatabase?parseTime=true",
        "maxIdleConns": 10,
        "maxOpenConns": 10,
        "maxLifeTime": 3,
        "maxIdleTime": 3
    },
    "graphql": {
        "port": ":5005",
        "host": "localhost",
        "readTimeout": "10s",
        "writeTimeout": "10s",
        "BaseRoute": "/graphql",
        "debugMode": true
    },
    "websocket":{
        "port": ":5006",
        "host": "localhost",
        "readTimeout": "10s",
        "writeTimeout": "10s",
        "BaseRoute": "/ws",
        "debugMode": true,
        "workers": 10,
        "queueSize": 100,
        "preallocate": 1,
        "ioTimeout": "10s",
        "debugPprof": "",
        "maxMsgSize": 1024
    }
}